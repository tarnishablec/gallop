{
  "name": "@gallop/real",
  "version": "0.0.4",
  "description": "gallop real",
  "private": true,
  "main": "dist/index.umd.js",
  "module": "dist/index.esm.js",
  "types": "dist/index.d.ts",
  "sideEffect": false,
  "repository": {
    "type": "git",
    "url": "https://github.com/tarnishablec/gallop.git"
  },
  "exports": {
    "default": {
      "import": [
        "./dist/index.esm.js",
        "./src/index.ts"
      ],
      "require": "./dist/index.umd.js"
    }
  },
  "keywords": [
    "gallop",
    "real",
    "site"
  ],
  "author": {
    "name": "tarnishablec",
    "email": "tarnishablec@outlook.com"
  },
  "homepage": "",
  "license": "MIT",
  "directories": {
    "src": "src",
    "test": "__tests__"
  },
  "files": [
    "src",
    "dist"
  ],
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  },
  "buildOptions": {
    "bundler": "vite",
    "rollupOptions": {
      "input": {
        "main": "./src/index.html"
      }
    }
  },
  "dependencies": {
    "css-format-monaco": "^2.0.4",
    "monaco-editor": "^0.27.0",
    "rxjs": "^7.3.0",
    "uuid": "^8.3.2",
    "vite-plugin-monaco-editor": "^1.0.4"
  }
}
